    "lodash.debounce": "4.0.8",
    "mobx": "6.0.3",
    "mobx-react": "7.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "tsyringe": "4.3.0"
  },
  "precommit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
