    "graphql-request": "3.4.0",
    "lodash.debounce": "4.0.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "reflect-metadata": "0.1.13",
    "styled-components": "5.2.1",
    "tsyringe": "4.4.0"
