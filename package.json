    "tslint": "6.1.3",
    "tslint-react": "5.0.0",
    "typescript": "4.0.5",
    "webpack": "5.4.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "zip-webpack-plugin": "3.0.0"
